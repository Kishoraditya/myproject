[run]
source = .
omit = 
    */migrations/*
    */tests/*
    */tests.py
    */test_*.py
    */settings/*
    */wsgi.py
    */asgi.py
    manage.py
    */__pycache__/*
    */venv/*
    */static/*
    */media/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError

[html]
directory = htmlcov 